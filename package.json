{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "compressor",
  "title": "Compressor",
  "description": "Compress Images and Videos in one click for web",
  "icon": "extension-icon.png",
  "license": "MIT",
  "commands": [
    {
      "name": "compress-file",
      "title": "Compress File",
      "description": "Compress any selected file",
      "mode": "no-view",
      "subtitle": ""
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.103.0",
    "@raycast/utils": "^2.2.1",
    "ffmpeg-static": "^5.3.0",
    "file-type": "^21.1.1"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^2.0.4",
    "@types/node": "22.13.10",
    "@types/react": "19.0.10",
    "eslint": "^9.22.0",
    "prettier": "^3.5.3",
    "typescript": "^5.8.2"
  },
  "scripts": {
    "dev": "ray develop",
    "lint": "ray lint",
    "fix-lint": "ray lint --fix",
    "build": "ray build",
    "publish": "npx @raycast/api@latest publish",
    "prepublishOnly": "echo \"\\n\\nIt seems like you are trying to publish the Raycast extension to npm.\\n\\nIf you did intend to publish it to npm, remove the \\`prepublishOnly\\` script and rerun \\`npm publish\\` again.\\nIf you wanted to publish it to the Raycast Store instead, use \\`npm run publish\\` instead.\\n\\n\" && exit 1"
  },
  "author": "webistory",
  "platforms": [
    "Windows"
  ],
  "categories": [
    "Developer Tools",
    "Design Tools",
    "Web",
    "Media"
  ]
}
